{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-config-prettier"
    ],
    "rules": {
        "prettier": [
            true,
            {"singleQuote": true}
        ],
        "interface-name": [false],
        "no-console": [false]
    },
    "rulesDirectory": ["tslint-plugin-prettier"]
}